//7)A Simple Java program to demonstrate // method overriding in java
import java.io.*;
import java.util.*;
class Animal {
    void makeSound() {
        System.out.println("Animal is making a sound");
    }
}

class Cat extends Animal {
    void makeSound() {
        System.out.println("Meow");
    }
}

public class MethodOverriding {
    public static void main(String[] args) {
        Animal myAnimal = new Animal();
        myAnimal.makeSound();

        Cat myCat = new Cat();
        myCat.makeSound();
    }
}

